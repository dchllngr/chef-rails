; Maintained by Chef.  Local changes will be overwritten

cgi.fix_pathinfo = 0
memory_limit = 32M
post_max_size = 8M
upload_max_filesize = 2M
error_reporting = E_ALL & ~E_NOTICE
date.timezone = 'Europe/Moscow'
sendmail_path = <%= node['msmtp']['binary'] %> -a system -t
;session_save.path = /tmp
;mail.force_extra_parameters =
<% @options.sort_by { |key, _| key }.each do |directive, value| -%>
<%= "#{directive} = \"#{value}\"" %>
<% end -%>
